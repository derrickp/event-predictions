{
  "name": "event-predictions",
  "version": "0.1.0",
  "description": "A web app for creating predictions for people to make picks in of upcoming events.",
  "main": "index.js",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.5.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build",
    "build": "npm run compile && npm run webpack && sass && echo Build complete",
    "compile": "tsc -v && tsc && echo server compile complete",
    "webpack": "webpack",
    "watch": "webpack -w",
    "server-watch": "tsc -v && tsc -w",
    "start": "node app.js",
    "start-dev": "nodemon app.js",
    "sass": "node-sass \"client/styles.scss\" -o \"public/dist\"",
    "sass-watch": "node-sass -w \"client/styles.scss\" -o \"public/dist\""
  },
  "author": "Derrick Plotsky <derrickplotsky@gmail.com>",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "@types/gapi.auth2": "^0.0.45",
    "@types/jest": "^21.1.4",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.25",
    "@types/koa-json": "^2.0.17",
    "@types/koa-jwt": "^3.2.2",
    "@types/koa-router": "^7.0.23",
    "@types/koa-static": "^3.0.1",
    "@types/material-ui": "^0.18.4",
    "@types/mongodb": "^2.2.15",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/react-redux": "^5.0.12",
    "@types/react-router": "^4.0.17",
    "@types/react-router-dom": "^4.2.1",
    "google-auth-library": "^0.11.0",
    "jest": "^21.2.1",
    "jsonwebtoken": "^8.1.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.2.2",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "material-ui": "^0.19.4",
    "mongodb": "^2.2.33",
    "node-sass": "^4.6.1",
    "nodemon": "^1.12.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "ts-jest": "^21.1.3",
    "ts-loader": "^3.0.5",
    "typescript": "^2.5.3",
    "webpack": "^3.8.1"
  }
}
